{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },

        {
           "type": "package",
           "package": {
                "name": "kenwheeler/slick",
                "version": "v1.8.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/kenwheeler/slick",
                    "type": "git",
                    "reference": "v1.8.1"
                }
           }
        },

        {
            "type": "package",
            "package": {
                "name": "lazysizes",
                "version": "5.1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/aFarkas/lazysizes/archive/5.1.1.zip",
                    "type": "zip"
                }
            }
        },

        {
           "type": "package",
           "package": {
                "name": "FrDH/mmenu-js",
                "version": "v8.5.24",
                "type": "drupal-library",
                "extra": {
                  "installer-name": "mmenu"
                },

                "source": {
                    "url": "https://github.com/FrDH/mmenu-js",
                    "type": "git",
                    "reference": "v8.5.24"
                }
           }
        }
    ],
    "require": {
        "FrDH/mmenu-js": "^8.5",
        "composer/installers": "^1.2",
        "drupal/admin_toolbar": "^2.4",
        "drupal/audiofield": "^1.10",
        "drupal/back_to_top": "^1.1",
        "drupal/better_formats": "1.x-dev@dev",
        "drupal/betterlogin": "^1.5",
        "drupal/blazy": "^2.4",
        "drupal/block_class": "^1.3",
        "drupal/blog": "^3.0",
        "drupal/captcha": "^1.2",
        "drupal/colorbox": "^1.7",
        "drupal/config_update": "^1.7",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/counter_up": "1.x-dev@dev",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.7",
        "drupal/customerror": "^1.0@alpha",
        "drupal/date_popup": "^1.1",
        "drupal/devel": "^4.1",
        "drupal/disable_messages": "^2.0",
        "drupal/dropzonejs": "^2.5",
        "drupal/dynamic_entity_reference": "^1.12",
        "drupal/embed": "^1.4",
        "drupal/empty_page": "^3.0",
        "drupal/entity": "^1.2",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_embed": "^1.1",
        "drupal/extlink": "^1.6",
        "drupal/fb_likebox": "^2.1",
        "drupal/features": "^3.12",
        "drupal/field_permissions": "^1.1",
        "drupal/file_entity": "^2.0@beta",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/flag": "^4.0@beta",
        "drupal/floating_block": "1.x-dev@dev",
        "drupal/flood_unblock": "^3.2",
        "drupal/fontawesome": "^2.19",
        "drupal/footable": "^1.0@alpha",
        "drupal/google_analytics": "^3.1",
        "drupal/htmlmail": "^3.0@alpha",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imce": "^2.4",
        "drupal/indic_script": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jcarousel": "^4.0",
        "drupal/juicebox": "^3.0@alpha",
        "drupal/lang_dropdown": "^2.0@beta",
        "drupal/libraries": "^3.0@beta",
        "drupal/login_destination": "^2.0@beta",
        "drupal/mailchimp": "^2.0",
        "drupal/mailsystem": "^4.3",
        "drupal/media_entity": "^2.0@beta",
        "drupal/media_entity_image": "^1.3",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/mmenu": "1.x-dev@dev",
        "drupal/module_filter": "^3.2",
        "drupal/module_missing_message_fixer": "^1.2",
        "drupal/nice_menus": "^1.0@beta",
        "drupal/node_export": "^1.0@alpha",
        "drupal/oauth": "^2.2",
        "drupal/page_manager": "^4.0@beta",
        "drupal/pathauto": "^1.8",
        "drupal/pdf_reader": "^1.0",
        "drupal/php": "^1.1",
        "drupal/poll": "^1.4",
        "drupal/push_notifications": "1.x-dev@dev",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/recaptcha": "^3.0",
        "drupal/responsive_menus": "^1.0@alpha",
        "drupal/responsive_tables_filter": "^1.8",
        "drupal/rpt": "^1.0",
        "drupal/rules": "^3.0@alpha",
        "drupal/scroll_to_top": "0.x-dev@dev",
        "drupal/search_api": "^1.20",
        "drupal/search_api_solr": "^4.2",
        "drupal/semiclean": "1.x-dev@dev",
        "drupal/shorten": "^1.0",
        "drupal/simple_pass_reset": "^1.1",
        "drupal/slick": "^2.3",
        "drupal/slick_media": "3.x-dev@dev",
        "drupal/smtp": "^1.0",
        "drupal/spambot": "^1.0",
        "drupal/superfish": "^1.4",
        "drupal/token": "^1.9",
        "drupal/transaction": "^1.0@beta",
        "drupal/twitter": "1.x-dev@dev",
        "drupal/unique_field": "2.x-dev@dev",
        "drupal/userpoints": "^1.0@beta",
        "drupal/userswitch": "^1.9",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_slideshow": "^4.8",
        "drupal/views_templates": "^1.1",
        "drupal/votingapi": "^3.0@beta",
        "drupal/webform": "^6.1@beta",
        "drupal/workbench": "^1.3",
        "drupal/workbench_moderation": "^1.6",
        "drupal/workflow": "^1.5",
        "drupal/xmlsitemap": "^1.0",
        "drupal/yamlform": "^1.0@beta",
        "drupal/youtube": "^1.2",
        "drupal/youtubechannel": "^3.2",
        "kenwheeler/slick": "^1.8",
        "lazysizes": "^5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}

